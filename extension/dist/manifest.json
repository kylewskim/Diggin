{
  "manifest_version": 3,
  "name": "Diggin",
  "version": "1.0.0",
  "description": "A Chrome extension for Diggin",
  "key": "",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "permissions": [
    "storage",
    "identity",
    "tabs",
    "clipboardRead"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "oauth2": {
    "client_id": "175967632049-v617c2uu6d3me1o4ititk2bb6uuvm624.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
} 